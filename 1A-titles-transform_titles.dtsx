      ,[Answer Code Text]
      ,CAST(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE([ISBN Name],'&amp;','&amp;amp;'),'''','&amp;apos;'),'&gt;','&amp;gt;'),'&lt;','&amp;lt;'),'"','&amp;quot;') AS VARCHAR(500)) AS [title_long_desc]
      --,CAST(SUBSTRING(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE([ISBN Name],'&amp;','&amp;amp;'),'''','&amp;apos;'),'&gt;','&amp;gt;'),'&lt;','&amp;lt;'),'"','&amp;quot;'),1,20) AS VARCHAR(20)) AS [title_short_desc]
	  , CASE WHEN CHARINDEX('''',SUBSTRING([ISBN Name],1,20)) &gt; 0
	          OR CHARINDEX('&amp;',SUBSTRING([ISBN Name],1,20)) &gt; 0
	          OR CHARINDEX('"',SUBSTRING([ISBN Name],1,20)) &gt; 0
	          OR CHARINDEX('&gt;',SUBSTRING([ISBN Name],1,20)) &gt; 0
	          OR CHARINDEX('&lt;',SUBSTRING([ISBN Name],1,20)) &gt; 0
