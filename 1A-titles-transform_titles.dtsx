      ,[SE publication date] AS [se_publication_date]
      ,[SE publication price] AS [se_publication_price]
	  ,CAST(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE([Author Artist],'&amp;','&amp;amp;'),'''','&amp;apos;'),'&gt;','&amp;gt;'),'&lt;','&amp;lt;'),'"','&amp;quot;') AS VARCHAR(500)) AS [author]
