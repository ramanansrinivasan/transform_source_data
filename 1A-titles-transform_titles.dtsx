			ELSE SUBSTRING(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE([ISBN Name],'&amp;','&amp;amp;'),'''','&amp;apos;'),'&gt;','&amp;gt;'),'&lt;','&amp;lt;'),'"','&amp;quot;'),1,20) END AS [title_short_desc]
      ,[Original Pub Date of Product]
