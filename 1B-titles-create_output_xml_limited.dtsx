      <DTS:Property
        DTS:DataType="8"
        DTS:Name="ParameterValue">SELECT '&lt;UC_PART_INB_IFD&gt;' +CHAR(13)+ STRING_AGG(CONVERT(VARCHAR(MAX), tit.resultant_payload),CHAR(13)) +CHAR(13)+ '&lt;/UC_PART_INB_IFD&gt;' AS resultant_payload FROM final.titles tit LEFT JOIN config.wms_dm_run_ctl ctl ON tit.run_id = ctl.run_id CROSS JOIN config.data_element_payload_fillers de WHERE ctl.package_name = 'titles'    AND ctl.run_status = 'SentToFile'    AND de.data_element_name = 'title' AND tit.record_number &gt;20000 AND tit.record_number &lt;= 25000--AND tit.ISBN_SuppEd IN ( SELECT TOP 20 ILV.ISBN_SuppEd FROM ( SELECT distinct t.ISBN_SuppEd, CAST(inv.[No-of-Units-1] + inv.[No-of-Units-2] AS INTEGER) as no_of_units   FROM final.titles t   JOIN final.inventories inv ON t.ISBN_SuppEd = inv.ISBN_SuppEd  WHERE t.PackSize &gt; 0 AND CAST(inv.[No-of-Units-1] AS INTEGER) + CAST(inv.[No-of-Units-2] AS INTEGER) &gt; 0    AND BaseLuId &lt;&gt; '') ilv);</DTS:Property>
    </DTS:PackageParameter>
