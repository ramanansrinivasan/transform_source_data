                <component
                  refId="Package\Transform Inventory Data\Transform inventories\Read Inventory Records"
                  componentClassID="Microsoft.OLEDBSource"
                  contactInfo="OLE DB Source;Microsoft Corporation; Microsoft SQL Server; (C) Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;7"
                  description="OLE DB Source"
                  name="Read Inventory Records"
                  usesDispositions="true"
                  version="7">
                  <properties>
                    <property
                      dataType="System.Int32"
                      description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                      name="CommandTimeout">0</property>
                    <property
                      dataType="System.String"
                      description="Specifies the name of the database object used to open a rowset."
                      name="OpenRowset"></property>
                    <property
                      dataType="System.String"
                      description="Specifies the variable that contains the name of the database object used to open a rowset."
                      name="OpenRowsetVariable"></property>
                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=16.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT tt.[run_id]
      ,tt.[Rec-Sta]
      ,tt.[Site]
      ,tt.[Placement]
      ,tt.[Location]
      ,tt.[Seg-Number]
      ,tt.[LU-per-segment]
      ,tt.[LU-in-segment]
      ,tt.[LU-Await-In]
      ,tt.[Seg-LU-Type]
      ,tt.[Segment-Status]
	  --,tt.[ISBN]
	  --,tt.[SupplyEd]
      ,tt.[ISBN_SuppEd]
      ,tt.[LPN]
      ,tt.[Impression-No]
      ,CASE WHEN tt.[Received-Date] LIKE '1900-01-01' THEN '' ELSE tt.[Received-Date] END AS [ReceivedDate]
      ,tt.[Bind-Order-No]
      ,tt.[Delivery-No]
      ,tt.[Unit-Price]
      ,tt.[Product-Price]
      ,tt.[Unit-Size]
      ,tt.[Unit-Weight]
      ,tt.[Base-Dim-Long]
      ,tt.[Base-Dim-Short]
      ,tt.[Unit-Dim-High]
      ,tt.[No-of-Units-1]
      ,tt.[Frozen-Code-1]
      ,tt.[No-of-Units-2]
      ,tt.[Frozen-Code-2]
      ,tt.[No-of-Units-3]
      ,tt.[Frozen-Code-3]
      ,tt.[No-of-Units-4]
      ,tt.[Frozen-Code-4]
      ,tt.[No-of-Units-5]
      ,tt.[Frozen-Code-5]
      ,tt.[No-of-Units-6]
      ,tt.[Frozen-Code-6]
      ,tt.[Opened-Date]
      ,tt.[No-Of-Accesses]
      ,tt.[Last-Transfer-Date]
      ,tt.[Last-Transfer-Qty]
      ,tt.[CI-Count-In]
      ,tt.[CI-Count-Out]
      ,tt.[CI-Count-Date]
      ,tt.[CI-Count-Total]
      ,tt.[CI-Count-Adjust]
      ,tt.[PI-Count-In]
      ,tt.[PI-Count-Out]
      ,tt.[PI-Count-Date]
      ,tt.[PI-Count-Total]
      ,tt.[PI-Count-Adjust]
      ,tt.[Hold-Unit-Size]
      ,tt.[Hold-Unit-Weight]
      ,tt.[Hold-Dim-Long]
      ,tt.[Hold-Dim-Short]
      ,tt.[Hold-Dim-High]
      ,tt.[Full-Pallet-Extr]
      ,tt.[PI-Count-Requested]
      ,tt.[PI-Count-QC-Check]
      ,tt.[Free-Locn-Flag]
      ,tt.[Transfer-Date]
      ,tt.[Cumulative-Accesses]
      ,tt.[PI-Generated-Date]
      ,tt.[In-Progress]
      ,tt.[Pick-Access-Sta]
      ,tt.[Repl-Access-Sta]
      ,tt.[Release-When-Empty]
	  ,tt.[PackSize]
      ,CAST([by_payload_filler] AS VARCHAR(8000)) AS [by_payload_filler]
     ,CAST(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(
      REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(--REPLACE(REPLACE( 
      REPLACE(by_payload_filler, '&lt;filler_prtnum&gt;', tt.[ISBN_SuppEd]) 
                                ,'&lt;filler_untcas&gt;', CASE WHEN tt.[PackSize] = 0 THEN 1 ELSE tt.[PackSize] END) --clarify whether mandatory, if mandatory, find mapping from Dan.
                                ,'&lt;filler_invsts&gt;', CASE WHEN tt.[Frozen-Code-2] = '' THEN 'A' ELSE tt.[Frozen-Code-2] END)--tt.[FrozenCode]) -- remove hardcoding once we have new datacut.
                                ,'&lt;filler_untqty&gt;', tt.[No-of-Units-1] + tt.[No-of-Units-2]) --clarify with Dan if it has been converted to eaches.
                                ,'&lt;filler_orgcod&gt;', '') --clarify whether mandatory, if mandatory, find mapping from Dan.
                                ,'&lt;filler_dstloc&gt;', tt.[Location]) 
                                ,'&lt;filler_lodnum&gt;', tt.[LPN]) --clarify whether mandatory, if mandatory, find mapping from Dan.
                                ,'&lt;filler_ftpcod&gt;', tt.[ISBN]) 
                               --,'&lt;filler_untpak&gt;', 0) --clarify whether mandatory, if mandatory, find mapping from Dan.
                                ,'&lt;filler_fifdte&gt;', CASE WHEN REPLACE(CONVERT(VARCHAR,CONVERT(DATE,tt.[Received-date], 103),112) ,'19000101','') &lt;&gt; '' THEN REPLACE(CONVERT(VARCHAR,CONVERT(DATE,tt.[Received-date], 103),112) ,'19000101','') + '000000' ELSE '' END) -- as per mapping
                                ,'&lt;filler_suppnum&gt;', '') --clarify whether mandatory, if mandatory, find mapping from Dan.
                                ,'&lt;filler_assettyp&gt;', '')--clarify whether mandatory, if mandatory, find mapping from Dan.
								AS VARCHAR(8000))
								AS resultant_payload
  FROM (
SELECT stg.[run_id]
      ,stg.[Rec-Sta]
      ,stg.[Site]
      ,stg.[Placement]
      ,stg.[Location]
