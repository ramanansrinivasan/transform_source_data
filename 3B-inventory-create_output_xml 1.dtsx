      <DTS:Property
        DTS:DataType="8"
        DTS:Name="ParameterValue">SELECT '&lt;CREATE_INVENTORY_IN_IFD&gt;' +CHAR(13)+ STRING_AGG(CONVERT(VARCHAR(MAX), inv.resultant_payload),CHAR(13)) +CHAR(13)+ '&lt;/CREATE_INVENTORY_IN_IFD&gt;' AS resultant_payload    FROM final.inventories_new inv    LEFT JOIN config.wms_dm_run_ctl ctl ON inv.run_id = ctl.run_id   CROSS JOIN config.data_element_payload_fillers de     WHERE ctl.package_name = 'inventory'      AND de.data_element_name = 'inventory' AND inv.lpn &lt;&gt; '' AND inv.ISBN_SuppEd IN ( SELECT TOP 20 ILV.ISBN_SuppEd  FROM ( SELECT distinct t.ISBN_SuppEd, CAST(inv.[No-of-Units-1] + inv.[No-of-Units-2] AS INTEGER) as no_of_units   FROM final.titles t   JOIN final.inventories_new inv ON t.ISBN_SuppEd = inv.ISBN_SuppEd  WHERE t.PackSize &gt; 0 AND CAST(inv.[No-of-Units-1] + inv.[No-of-Units-2] AS INTEGER) &gt; 0    AND LPN &lt;&gt; '') ilv);</DTS:Property>
    </DTS:PackageParameter>
  </DTS:PackageParameters>
